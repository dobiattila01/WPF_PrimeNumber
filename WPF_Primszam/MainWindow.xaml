<Window x:Class="WPF_Primszam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Primszam"
        mc:Ignorable="d"
        Title="Primgenerator" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">            
            <TextBox x:Name="textBox" Width="Auto" Height="Auto" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top"/>
        </Grid>
        <Grid Grid.Row="1">

            <Grid Grid.Column="0">
                <Button x:Name="generatePrime_button" Content="Generate" Height="30" Margin="5" Click="generatePrime_button_Click" HorizontalAlignment="Left" VerticalAlignment="Top"></Button>
                <TextBox x:Name="generatedPrimeElement_textbox" HorizontalAlignment="Stretch" Height="auto" Margin="64,10,354.4,62.2" TextWrapping="Wrap" Width="100" />
            </Grid>
        </Grid>
        
    </Grid>
</Window>
